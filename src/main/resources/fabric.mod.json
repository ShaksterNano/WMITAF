{
  "schemaVersion": 1,
  "id": "wmitef",
  "version": "${version}",

  "name": "WMITEF",
  "description": "What Mod Is This Enchantment From?\nAn addon for WTHIT and REI that shows the name of the mod that adds an enchantment in the tooltip of an enchanted book instead of \"Minecraft\".",
  "authors": [
    "ShaksterNano"
  ],
  "contact": {
    "sources": "https://github.com/ShakterNano/WMITEF",
    "issues": "https://github.com/ShakterNano/WMITEF/issues"
  },

  "license": "MIT",
  "icon": "assets/wmitef/icon.png",

  "environment": "client",
  "mixins": [
    "wmitef.mixins.json"
  ],

  "custom": {
    "waila:plugins": {
      "id": "wmitef:waila_plugin",
      "initializer": "io.github.shaksternano.wmitef.client.plugin.waila.WmitefWailaPlugin"
    }
  },

  "depends": {
    "fabricloader": ">=0.11.0",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "wthit": ">=3.11.0",
    "roughlyenoughitems": ">=6.1.315"
  }
}
