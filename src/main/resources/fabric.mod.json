{
  "schemaVersion": 1,
  "id": "wmitef",
  "version": "${version}",

  "name": "WMITEF",
  "description": "What Mod Is This Enchantment From?\nAn addon for WTHIT that shows the name of the mod that adds an enchantment under an enchanted book instead of \"Minecraft\".",
  "authors": [
    "ShaksterNano"
  ],
  "contact": {
    "sources": "https://github.com/ShakterNano/WMITEF",
    "issues": "https://github.com/ShakterNano/WMITEF/issues"
  },

  "license": "MIT",
  "icon": "assets/wmitef/icon.png",

  "environment": "client",

  "custom": {
    "waila:plugins": {
      "id": "wmitef:waila_plugin",
      "initializer": "io.github.shaksternano.wmitef.client.plugin.waila.WmitefWailaPlugin"
    }
  },

  "depends": {
    "fabricloader": ">=0.11.0",
    "minecraft": "1.17.x",
    "java": ">=16",
    "wthit": ">=3.11.0"
  }
}
